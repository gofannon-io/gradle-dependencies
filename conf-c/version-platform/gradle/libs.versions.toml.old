[versions]
junit = "5.9.2"
annotations = "24.0.0"
assertj = "3.24.2"
copyrighter = "2.0"
personProvider = "1.0"
translator = "1.0"

[libraries]
annotations = { module = "org.jetbrains:annotations", version.ref = "annotations" }

junit-api = { module = "org.junit.jupiter:junit-jupiter-api", version.ref = "junit" }
junit-engine = { module = "org.junit.jupiter:junit-jupiter-engine", version.ref = "junit" }
junit-params = { module = "org.junit.jupiter:junit-jupiter-params", version.ref = "junit" }
assertj-core = { module = "org.assertj:assertj-core", version.ref = "assertj" }

copyrighter = { module = "io.gofannon.waterfall:copyrighter", version.ref = "copyrighter" }
person-provider = { module = "io.gofannon.waterfall:person-provider", version.ref = "personProvider" }
translator = { module = "io.gofannon.waterfall:translator", version.ref = "translator" }


[bundles]
#groovy = ["groovy-core", "groovy-json", "groovy-nio"]
testImpl=["junit-api", "junit-params", "assertj-core"]

